<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/dimen_427_dip"
    android:layout_height="@dimen/dimen_413_dip"
    android:layout_gravity="center"
    android:layout_marginLeft="@dimen/dimen_20_dip"
    android:layout_marginRight="@dimen/dimen_20_dip"
    android:background="@drawable/sdk_main_bg"
    android:orientation="vertical" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="55dp" >

        <include layout="@layout/mzyw_pp_title" />

        <include layout="@layout/mzyw_pp_logo" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="2dp"
        android:layout_marginRight="2dp"
        android:background="#d6d6d6" />

    <LinearLayout
        android:id="@+id/login_name_layout"
        android:layout_width="@dimen/dimen_395_dip"
        android:layout_height="@dimen/dimen_59_dip"
        android:layout_marginLeft="@dimen/dimen_16_dip"
        android:layout_marginRight="@dimen/dimen_16_dip"
        android:layout_marginTop="@dimen/dimen_33_dip"
        android:background="@drawable/txt_while"
        android:orientation="horizontal" >

        <LinearLayout 
            android:layout_width="@dimen/dimen_45_dip"
            android:layout_height="@dimen/dimen_59_dip"
            android:gravity="center" >
            <ImageView
	            android:id="@+id/image_change"
	            android:layout_width="@dimen/dimen_29_dip"
	            android:layout_height="@dimen/dimen_29_dip"
	            android:src="@drawable/vip_0" />
        </LinearLayout>

        <TextView
            android:id="@+id/qstart_name_tv"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="0.91"
            android:gravity="center_vertical|left"
            android:textColor="@color/mz_text_thj"
            android:textSize="@dimen/dimen_17_dip" />

        <ImageView
            android:id="@+id/qstart_more_account"
            android:layout_width="@dimen/dimen_25_dip"
            android:layout_height="@dimen/dimen_25_dip"
            android:layout_gravity="center_vertical"
            android:layout_marginRight="5dp"
            android:padding="@dimen/dimen_6_dip"
            android:src="@drawable/qstart_more_btn1" />
    </LinearLayout>

    <Button
        android:id="@+id/qstart_login_bt"
        android:layout_width="@dimen/dimen_395_dip"
        android:layout_height="@dimen/dimen_52_dip"
        android:layout_marginLeft="@dimen/dimen_16_dip"
        android:layout_marginRight="@dimen/dimen_16_dip"
        android:layout_marginTop="@dimen/dimen_20_dip"
        android:background="@drawable/btn_08" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dimen_16_dip"
        android:layout_marginRight="@dimen/dimen_16_dip"
        android:layout_marginTop="@dimen/dimen_27_dip"
        android:src="@drawable/pic_03" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dimen_16_dip"
        android:layout_marginRight="@dimen/dimen_16_dip"
        android:layout_marginTop="@dimen/dimen_20_dip"
        android:weightSum="3" >

        <LinearLayout
            android:id="@+id/liny_001"
            android:layout_width="@dimen/dimen_50_dip"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dimen_7_dip"
            android:layout_weight="1"
            android:orientation="vertical" >

            <ImageView
                android:id="@+id/qstart_bind_iv_image"
                android:layout_width="@dimen/dimen_33_dip"
                android:layout_height="@dimen/dimen_33_dip"
                android:layout_gravity="center"
                android:src="@drawable/huikouzhen" />

            <TextView
                android:id="@+id/qstart_bind_iv_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="@dimen/dimen_11_dip"
                android:text="账号升级"
                android:textColor="#333333"
                android:textSize="@dimen/dimen_16_dip" />
        </LinearLayout>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="2dp"
            android:layout_marginRight="2dp"
            android:background="#ebebeb" />

        <LinearLayout
            android:id="@+id/liny_002"
            android:layout_width="@dimen/dimen_50_dip"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_weight="1"
            android:orientation="vertical" >

            <ImageView
                android:id="@+id/qstart_cpsd_iv"
                android:layout_width="@dimen/dimen_33_dip"
                android:layout_height="@dimen/dimen_33_dip"
                android:layout_gravity="center"
                android:src="@drawable/bangding_ccc" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="修改密码"
                android:textColor="#333333"
                android:layout_marginTop="@dimen/dimen_11_dip"
                android:textSize="@dimen/dimen_16_dip" />
        </LinearLayout>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="2dp"
            android:layout_marginRight="2dp"
            android:background="#ebebeb" />

        <LinearLayout
            android:id="@+id/liny_003"
            android:layout_width="@dimen/dimen_50_dip"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:layout_marginRight="@dimen/dimen_7_dip"
            android:layout_weight="1"
            android:orientation="vertical" >

            <ImageView
                android:id="@+id/qstart_susn_iv"
                android:layout_width="@dimen/dimen_33_dip"
                android:layout_height="@dimen/dimen_33_dip"
                android:layout_gravity="center"
                android:src="@drawable/zuoyouhuhuanjiantou" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="切换账号"
                android:textColor="#333333"
                android:layout_marginTop="@dimen/dimen_11_dip"
                android:textSize="@dimen/dimen_16_dip" />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>