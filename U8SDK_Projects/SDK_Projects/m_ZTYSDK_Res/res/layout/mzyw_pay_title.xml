<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dimen_73_dip"
    android:layout_gravity="center"
    android:orientation="horizontal" >

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="@dimen/dimen_177_dip"
        android:layout_height="@dimen/dimen_56_dip"
        android:layout_centerInParent="true"
        android:src="@drawable/muzhitongxingzheng_logo" />

    <ImageView
        android:id="@+id/close"
        android:layout_width="@dimen/dimen_40_dip"
        android:layout_height="@dimen/dimen_37_dip"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/back"
        android:layout_marginRight="@dimen/dimen_16_dip"
        android:src="@drawable/ch" />

    <ImageView
        android:id="@+id/back"
        android:layout_width="@dimen/dimen_40_dip"
        android:layout_height="@dimen/dimen_37_dip"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:layout_marginLeft="@dimen/dimen_16_dip"
        android:src="@drawable/zjt" />

</RelativeLayout>